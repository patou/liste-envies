    "bump-patch-snapshot": "npm --no-git-tag-version version patch && node snapshot.js",
    "bump-unsnapshot": "node snapshot.js --remove",
    "preversion": "npm test",
    "version": "npm run distribution && git add -A dist",
    "postversion": "git push && git push --tags"
  },
