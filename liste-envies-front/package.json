{
  "name": "liste-envies-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,md,ts,html,component.html}\" !**/documentation/**",
    "precommit": "lint-staged",
    "start": "ng serve --proxy-config proxy-config.local.json",
    "serve": "ng serve --proxy-config proxy-config.dist.json --port 4242",
    "serve-prod": "ng serve --proxy-config proxy-config.dist.json --port 4240 --prod",
    "ce": "ng serve --proxy-config proxy-config.dist.json --host 0.0.0.0 --port 8003 --disable-host-check",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "hmr": "ng serve --hmr --proxy-config proxy-config.dist.json --host 0.0.0.0 --port 8004 --disable-host-check"
  },
  "private": true,
  "dependencies": {
